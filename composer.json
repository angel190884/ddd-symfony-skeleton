{
  "name": "sti-solutions/ddd-skeleton",
  "license": "MIT",
  "type": "project",
  "description": "StiSolutions backend",
  "require": {
    "php": ">=7.4.16",

    "ext-apcu": "*",
    "ext-json": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",

    "symfony/framework-bundle": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/yaml": "5.2.*",
    "symfony/orm-pack": "^2.1",
    
    "friendsofsymfony/rest-bundle": "^3.0",
    "symfony/serializer-pack": "^1.0"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "roave/security-advisories": "dev-master",

    "behat/behat": "^3.8",
    "friends-of-behat/symfony-extension": "^2.1",
    "behatch/contexts": "^3.3",
    "friends-of-behat/mink-browserkit-driver": "^1.5",

    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "StiSolutions\\Apps\\Scuti\\Backend\\": "apps/scuti/backend/src",
      "StiSolutions\\Apps\\Scuti\\Frontend\\": "apps/scuti/frontend/src",

      "StiSolutions\\Apps\\Imovit\\Backend\\": "apps/imovit/backend/src",
      "StiSolutions\\Apps\\Imovit\\Frontend\\": "apps/imovit/frontend/src",

      "StiSolutions\\Apps\\Telia\\Backend\\": "apps/telia/backend/src",
      "StiSolutions\\Apps\\Telia\\Frontend\\": "apps/telia/frontend/src",

      "StiSolutions\\": ["src"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StiSolutions\\Tests\\": ["tests/src"]
    }
  }
}
